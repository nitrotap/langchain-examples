351300***********************************************************
351400*
351500*    IF P-N-SCH-REBASED-FY90 OR
351600*       P-N-EACH OR
351700*       P-N-MDH-REBASED-FY90
351800*        PERFORM 3450-CALC-ADDITIONAL-HSP.
351900*
352000     MOVE 00                 TO  PPS-RTC.
352100     MOVE H-WAGE-INDEX       TO  PPS-WAGE-INDX.
352200     MOVE H-ALOS             TO  PPS-AVG-LOS.
352300     MOVE H-DAYS-CUTOFF      TO  PPS-DAYS-CUTOFF.
352400
352500     MOVE B-LOS TO H-PERDIEM-DAYS.
352600     IF H-PERDIEM-DAYS < 1
352700         MOVE 1 TO H-PERDIEM-DAYS.
352800     ADD 1 TO H-PERDIEM-DAYS.
352900
353000     MOVE 1 TO H-DSCHG-FRCTN.
353100
353200     COMPUTE H-DRG-WT-FRCTN ROUNDED = H-DSCHG-FRCTN * H-DRG-WT.
353300
353400     IF (PAY-PERDIEM-DAYS  OR
353500         PAY-XFER-NO-COST) OR
353600        (PAY-XFER-SPEC-DRG AND
353700         B-DRG-POSTACUTE-PERDIEM)
353800         COMPUTE H-TRANSFER-ADJ ROUNDED = H-PERDIEM-DAYS / H-ALOS
353900         COMPUTE H-DSCHG-FRCTN  ROUNDED = H-PERDIEM-DAYS / H-ALOS
354000         IF H-DSCHG-FRCTN > 1
354100              MOVE 1 TO H-DSCHG-FRCTN
354200              MOVE 1 TO H-TRANSFER-ADJ
354300         ELSE
354400              COMPUTE H-DRG-WT-FRCTN ROUNDED =
354500                  (H-PERDIEM-DAYS / H-ALOS) * H-DRG-WT.
354600
354700     IF (PAY-XFER-SPEC-DRG AND
354800         B-DRG-POSTACUTE-50-50)
354900         COMPUTE H-TRANSFER-ADJ ROUNDED = H-PERDIEM-DAYS / H-ALOS
355000         COMPUTE H-DSCHG-FRCTN  ROUNDED =
355100                        .5 + ((.5 * H-PERDIEM-DAYS) / H-ALOS)
355200         IF H-DSCHG-FRCTN > 1
355300              MOVE 1 TO H-DSCHG-FRCTN
355400              MOVE 1 TO H-TRANSFER-ADJ
355500         ELSE
355600              COMPUTE H-DRG-WT-FRCTN ROUNDED =
355700            (.5 + ((.5 * H-PERDIEM-DAYS) / H-ALOS)) * H-DRG-WT.
355800
355900
356000***********************************************************
356100***  CAPITAL DSH CALCULATION
356200***  CAPITAL DSH CALCULATION
356300
356400     MOVE 0 TO H-CAPI-DSH.
356500
356600     IF P-NEW-BED-SIZE NOT NUMERIC
356700         MOVE 0 TO P-NEW-BED-SIZE.
356800
356900     IF (W-CBSA-SIZE = 'O' OR 'L') AND P-NEW-BED-SIZE > 99
357000         COMPUTE H-CAPI-DSH ROUNDED = 2.7183 **
357100                  (.2025 * (P-NEW-SSI-RATIO
357200                          + P-NEW-MEDICAID-RATIO)) - 1.
