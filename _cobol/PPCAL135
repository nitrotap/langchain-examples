521900 7000-CALC-VALUE-BASED-PURCH.
522000***********************************************************
522100***** CASES INVOLVING VALUE BASE PURCH ADJUSTMENTS
522200***********************************************************
522300
522400     MOVE 0 TO H-VAL-BASED-PURCH-ADJUST-AMT.
522500
522600     IF  P-VAL-BASED-PURCH-PARTIPNT = 'N' OR 'Y'
522700           NEXT SENTENCE
522800     ELSE
522900           MOVE 68 TO PPS-RTC
523000           GO TO 7000-EXIT.
523100
523200     IF  P-VAL-BASED-PURCH-PARTIPNT = 'N'
523300           GO TO 7000-EXIT.
523400
523500     IF  P-VAL-BASED-PURCH-PARTIPNT = 'Y' AND
523600         P-NEW-CBSA-HOSP-QUAL-IND = '1'
523700           NEXT SENTENCE
523800     ELSE
523900           MOVE 68 TO PPS-RTC
524000           GO TO 7000-EXIT.
524100
524200     IF  P-VBP-INVALID-STATE
524300           MOVE 68 TO PPS-RTC
524400           GO TO 7000-EXIT
524500     ELSE
524600           NEXT SENTENCE.
524700
524800     IF P-VAL-BASED-PURCH-ADJUST > 0.9900000000 AND
524900        P-VAL-BASED-PURCH-ADJUST < 2.0000000000
525000           GO TO 7000-COMPUTE-VAL-BASED-PUR
525100     ELSE
525200           MOVE 68 TO PPS-RTC
525300           MOVE ZEROES TO H-VAL-BASED-PURCH-ADJUST-AMT.
525400           GO TO 7000-EXIT.
525500
525600 7000-COMPUTE-VAL-BASED-PUR.
525700
