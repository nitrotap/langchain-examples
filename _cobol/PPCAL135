353100     MOVE 00                 TO  PPS-RTC.
353200     MOVE H-WAGE-INDEX       TO  PPS-WAGE-INDX.
353300     MOVE H-ALOS             TO  PPS-AVG-LOS.
353400     MOVE H-DAYS-CUTOFF      TO  PPS-DAYS-CUTOFF.
353500
353600     MOVE B-LOS TO H-PERDIEM-DAYS.
353700     IF H-PERDIEM-DAYS < 1
353800         MOVE 1 TO H-PERDIEM-DAYS.
353900     ADD 1 TO H-PERDIEM-DAYS.
354000
354100     MOVE 1 TO H-DSCHG-FRCTN.
354200
354300     COMPUTE H-DRG-WT-FRCTN ROUNDED = H-DSCHG-FRCTN * H-DRG-WT.
354400
354500     IF (PAY-PERDIEM-DAYS  OR
354600         PAY-XFER-NO-COST) OR
354700        (PAY-XFER-SPEC-DRG AND
354800         B-DRG-POSTACUTE-PERDIEM)
354900         COMPUTE H-TRANSFER-ADJ ROUNDED = H-PERDIEM-DAYS / H-ALOS
355000         COMPUTE H-DSCHG-FRCTN  ROUNDED = H-PERDIEM-DAYS / H-ALOS
355100         IF H-DSCHG-FRCTN > 1
355200              MOVE 1 TO H-DSCHG-FRCTN
355300              MOVE 1 TO H-TRANSFER-ADJ
355400         ELSE
355500              COMPUTE H-DRG-WT-FRCTN ROUNDED =
355600                  (H-PERDIEM-DAYS / H-ALOS) * H-DRG-WT.
355700
355800     IF (PAY-XFER-SPEC-DRG AND
355900         B-DRG-POSTACUTE-50-50)
356000         COMPUTE H-TRANSFER-ADJ ROUNDED = H-PERDIEM-DAYS / H-ALOS
356100         COMPUTE H-DSCHG-FRCTN  ROUNDED =
356200                        .5 + ((.5 * H-PERDIEM-DAYS) / H-ALOS)
356300         IF H-DSCHG-FRCTN > 1
356400              MOVE 1 TO H-DSCHG-FRCTN
356500              MOVE 1 TO H-TRANSFER-ADJ
356600         ELSE
356700              COMPUTE H-DRG-WT-FRCTN ROUNDED =
356800            (.5 + ((.5 * H-PERDIEM-DAYS) / H-ALOS)) * H-DRG-WT.
356900
