346900*
347000     MOVE 00                 TO  PPS-RTC.
347100     MOVE H-WAGE-INDEX       TO  PPS-WAGE-INDX.
347200     MOVE H-ALOS             TO  PPS-AVG-LOS.
347300     MOVE H-DAYS-CUTOFF      TO  PPS-DAYS-CUTOFF.
347400
347500     MOVE B-LOS TO H-PERDIEM-DAYS.
347600     IF H-PERDIEM-DAYS < 1
347700         MOVE 1 TO H-PERDIEM-DAYS.
347800     ADD 1 TO H-PERDIEM-DAYS.
347900
348000     MOVE 1 TO H-DSCHG-FRCTN.
348100
348200     COMPUTE H-DRG-WT-FRCTN ROUNDED = H-DSCHG-FRCTN * H-DRG-WT.
348300
348400     IF (PAY-PERDIEM-DAYS  OR
348500         PAY-XFER-NO-COST) OR
348600        (PAY-XFER-SPEC-DRG AND
348700         B-DRG-POSTACUTE-PERDIEM)
348800         COMPUTE H-TRANSFER-ADJ ROUNDED = H-PERDIEM-DAYS / H-ALOS
348900         COMPUTE H-DSCHG-FRCTN  ROUNDED = H-PERDIEM-DAYS / H-ALOS
349000         IF H-DSCHG-FRCTN > 1
349100              MOVE 1 TO H-DSCHG-FRCTN
349200              MOVE 1 TO H-TRANSFER-ADJ
349300         ELSE
349400              COMPUTE H-DRG-WT-FRCTN ROUNDED =
349500                  (H-PERDIEM-DAYS / H-ALOS) * H-DRG-WT.
349600
349700     IF (PAY-XFER-SPEC-DRG AND
349800         B-DRG-POSTACUTE-50-50)
349900         COMPUTE H-TRANSFER-ADJ ROUNDED = H-PERDIEM-DAYS / H-ALOS
350000         COMPUTE H-DSCHG-FRCTN  ROUNDED =
350100                        .5 + ((.5 * H-PERDIEM-DAYS) / H-ALOS)
350200         IF H-DSCHG-FRCTN > 1
350300              MOVE 1 TO H-DSCHG-FRCTN
350400              MOVE 1 TO H-TRANSFER-ADJ
350500         ELSE
350600              COMPUTE H-DRG-WT-FRCTN ROUNDED =
350700            (.5 + ((.5 * H-PERDIEM-DAYS) / H-ALOS)) * H-DRG-WT.
350800
350900
