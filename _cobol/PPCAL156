497500     MOVE 1 TO H-DSCHG-FRCTN.
497600
497700     COMPUTE H-DRG-WT-FRCTN ROUNDED = H-DSCHG-FRCTN * H-DRG-WT.
497800
497900     IF (PAY-PERDIEM-DAYS  OR
498000         PAY-XFER-NO-COST) OR
498100        (PAY-XFER-SPEC-DRG AND
498200         D-DRG-POSTACUTE-PERDIEM)
498300       IF H-ALOS > 0
498400         COMPUTE H-TRANSFER-ADJ ROUNDED = H-PERDIEM-DAYS / H-ALOS
498500         COMPUTE H-DSCHG-FRCTN  ROUNDED = H-PERDIEM-DAYS / H-ALOS
498600         IF H-DSCHG-FRCTN > 1
498700              MOVE 1 TO H-DSCHG-FRCTN
498800              MOVE 1 TO H-TRANSFER-ADJ
498900         ELSE
499000              COMPUTE H-DRG-WT-FRCTN ROUNDED =
499100                  (H-PERDIEM-DAYS / H-ALOS) * H-DRG-WT
499200         END-IF
499300        END-IF
499400     END-IF.
499500
499600
499700     IF (PAY-XFER-SPEC-DRG AND
499800         D-DRG-POSTACUTE-50-50) AND
499900         H-ALOS > 0
500000         COMPUTE H-TRANSFER-ADJ ROUNDED = H-PERDIEM-DAYS / H-ALOS
500100         COMPUTE H-DSCHG-FRCTN  ROUNDED =
500200                        .5 + ((.5 * H-PERDIEM-DAYS) / H-ALOS)
500300         IF H-DSCHG-FRCTN > 1
500400              MOVE 1 TO H-DSCHG-FRCTN
500500              MOVE 1 TO H-TRANSFER-ADJ
500600         ELSE
500700              COMPUTE H-DRG-WT-FRCTN ROUNDED =
500800            (.5 + ((.5 * H-PERDIEM-DAYS) / H-ALOS)) * H-DRG-WT.
500900
501000
501100***********************************************************
501200***  CAPITAL DSH CALCULATION
501300***  CAPITAL DSH CALCULATION
501400
501500     MOVE 0 TO H-CAPI-DSH.
